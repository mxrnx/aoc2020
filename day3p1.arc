(def solve (h w)
  (if (>= h fin)
      0
      (let nw (if (>= w wrp)
		  (- w wrp)
		  w)
	(if (is ((chart h) nw) #\#)
	    (+ (solve (+ h 1) (+ nw 3)) 1)
	    (solve (+ h 1) (+ nw 3))))))

(def run-puzzle (e)
  (= chart (dump-puzzle-input 3 e)
     fin (len chart)
     wrp (len (car chart)))
  (solve 0 0))
